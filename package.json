{
  "name": "verifier",
  "version": "1.23.0",
  "private": true,
  "scripts": {
    "dev": "vite dev --host",
    "build": "paraglide-js compile --project ./project.inlang && vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check .",
    "format": "prettier --plugin-search-dir . --write .",
    "ionic:build": "npm run build",
    "ionic:serve": "npm run dev -- --host",
    "ios": "ionic cap run ios -l",
    "android": "ionic cap run android -l --external",
    "apk": "ionic capacitor copy android && cd android && ./gradlew assembleDebug && ./gradlew installDebug && cd ..",
    "web": "ionic serve",
    "postinstall": "paraglide-js compile --project ./project.inlang && cp -r ./node_modules/@didroom/components/dist/ ./static/components"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^7.0.1",
    "@didroom/components": "^1.32.2",
    "@inlang/paraglide-js": "1.2.5",
    "@inlang/paraglide-js-adapter-sveltekit": "^0.4.1",
    "@ionic/cli": "^7.2.0",
    "@playwright/test": "^1.50.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.3",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@sveltejs/kit": "^2.17.1",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@types/eslint": "8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "postcss": "^8.5.2",
    "postcss-load-config": "^5.1.0",
    "prettier": "^3.5.0",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "rollup-plugin-visualizer": "^5.14.0",
    "semantic-release": "^23.1.1",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vite": "^5.4.14",
    "vitest": "^1.6.1"
  },
  "type": "module",
  "dependencies": {
    "@capacitor/android": "^7.0.1",
    "@capacitor/app": "^7.0.0",
    "@capacitor/camera": "^7.0.0",
    "@capacitor/core": "^7.0.1",
    "@capacitor/device": "^7.0.0",
    "@capacitor/haptics": "^7.0.0",
    "@capacitor/ios": "^7.0.1",
    "@capacitor/keyboard": "^7.0.0",
    "@capacitor/network": "^7.0.0",
    "@capacitor/preferences": "^7.0.0",
    "@capacitor/push-notifications": "^7.0.0",
    "@capacitor/status-bar": "^7.0.0",
    "@fontsource-variable/gantari": "^5.1.1",
    "@fontsource/poppins": "^5.1.1",
    "@ionic/core": "^7.8.6",
    "@lottiefiles/svelte-lottie-player": "^0.3.1",
    "@slangroom/core": "^1.43.2",
    "@slangroom/helpers": "^1.43.2",
    "@slangroom/http": "^1.43.2",
    "@slangroom/location": "^1.43.2",
    "@slangroom/pocketbase": "^1.43.2",
    "@slangroom/qrcode": "^1.43.2",
    "@slangroom/zencode": "^1.43.2",
    "@sveltejs/adapter-static": "^3.0.8",
    "@types/lodash": "^4.17.15",
    "@vite-pwa/sveltekit": "^0.3.0",
    "capacitor-native-settings": "^6.0.5",
    "dayjs": "^1.11.13",
    "ionic-svelte": "^0.5.85",
    "ionicons": "^7.4.0",
    "lodash": "^4.17.21",
    "prettier-plugin-organize-imports": "^3.2.4",
    "svelte-preprocess": "^5.1.4",
    "sveltekit-superforms": "~1.13.4",
    "zenroom": "^4.47.0",
    "zod": "^3.24.1"
  }
}